<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="cloth" class="com.tnsif.SampleSpring.Clothes"/>
	
	<bean id="food" class="com.tnsif.SampleSpring.Food" />

	<bean id="customer1" class="com.tnsif.SampleSpring.Customer">
    <property name="id" value="101"/>
    <property name="name" value="Arun"/>
    <property name="city" value="Cuddalore"/>
    <property name="s1" ref="food"/> <!-- ✅ must be lowercase -->
</bean>

<bean id="customer2" class="com.tnsif.SampleSpring.Customer">
    <property name="id" value="102"/>
    <property name="name" value="Suja"/>
    <property name="city" value="Cuddalore"/>
    <property name="s1" ref="cloth"/> <!-- ✅ must be lowercase -->
</bean>

</beans>
